<mvc:View controllerName="ui5.camera.controller.Home"
    xmlns="sap.m"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns:l="sap.ui.layout"
    xmlns:form="sap.ui.layout.form"
    xmlns:core="sap.ui.core"
    xmlns:html="http://www.w3.org/1999/xhtml">
    <Page id="page" title="{i18n>title}">
        <content >

            <Image src="../assets/pln1.jpeg"></Image>
            <Button id="helloButton" text="Tesseract" press="onTess" />
            <Button icon="sap-icon://camera" type="Attention" text="Take a pic!" class="sapUiLargeMarginBegin" press="capturePic"/>
            <VBox id="vBox1" width="100%"></VBox>
        </content>
    </Page>
</mvc:View>
